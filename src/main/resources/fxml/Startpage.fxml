<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="main.java.app.controllers.StartpageController">
    <center>
        <AnchorPane>
            <children>
                <ImageView pickOnBounds="true" smooth="false">
                    <image>
                        <Image url="@../cards/tlo.jpg"/>
                    </image>
                </ImageView>
                <Button layoutX="309.0" layoutY="205.0" mnemonicParsing="false" onAction="#setPlayers" prefHeight="65.0"
                        prefWidth="103.0" text="New game"/>
                <Button cancelButton="true" layoutX="327.0" layoutY="343.0" mnemonicParsing="false" onAction="#exitGame"
                        prefHeight="31.0" prefWidth="67.0" text="Exit"/>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
